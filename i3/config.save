border_radius  9
gaps inner 9
gaps outer 5
gaps horizontal 4
exec_always yakuake
for_window [class="yakuake"] floating enable border none
for_window [class="Spotify"] border none gaps inner 0 gaps outer 0  



exec compton
bindsym $mod+Escape exec i3lock  -B 10  --indicator --keylayout 2 -k --insidecolor ffffff55 --line-uses-inside --ringcolor 00000055 --ringwrongcolor ff000055 --ringvercolor 0000ff55 --insidevercolor ffffff55 -k --insidewrongcolor ffffff55 --keyhlcolor 00ff0055 --bshlcolor ffff0055 --separatorcolor 00000000 --timestr="%H:%M"
bindsym $mod+Shift+w exec firefox
bindsym $mod+Shift+f exec dolphin
bindsym Print exec flameshot gui
bindsym $mod+t exec telegram-desktop
bindsym $mod+Shift+v exec vlc
bindsym $mod+Shift+s exec spotify
exec_always setxkbmap -layout us,ir && setxkbmap -option 'grp:alt_shift_toggle'
exec_always --no-startup-id feh --bg-fill ~/e.jpg
set $mod Mod4
font pango:Accuratist 9
exec --no-startup-id xss-lock --transfer-sleep-lock -- i3lock --nofork
exec --no-startup-id nm-applet
set $refresh_i3status killall -SIGUSR1 i3status
bindsym XF86AudioRaiseVolume exec --no-startup-id pactl set-sink-volume @DEFAULT_SINK@ +10% && $refresh_i3status
bindsym XF86AudioLowerVolume exec --no-startup-id pactl set-sink-volume @DEFAULT_SINK@ -10% && $refresh_i3status
bindsym XF86AudioMute exec --no-startup-id pactl set-sink-mute @DEFAULT_SINK@ toggle && $refresh_i3status
bindsym XF86AudioMicMute exec --no-startup-id pactl set-source-mute @DEFAULT_SOURCE@ toggle && $refresh_i3status
floating_modifier $mod
bindsym $mod+Return exec i3-sensible-terminal
